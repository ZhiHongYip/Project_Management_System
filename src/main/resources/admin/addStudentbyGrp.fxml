<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@../Admin_CSS/panefile.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="admin.addStudentbyGrp">
